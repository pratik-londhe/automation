        <class name="testscripts.regression.interactives.BiologyUnitOneNoInteractivesTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitTwoInteractivesTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitTwoNoInteractiveTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitFourInteractivesTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitOneInteractivesTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitFourNoInteractiveTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitThreeNoInteractivesTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitFiveNoInteractiveTest"/>
        <class name="testscripts.regression.interactives.BiologyUnitFiveInteractivesTest"></class>
    </classes>
</test>
</suite>